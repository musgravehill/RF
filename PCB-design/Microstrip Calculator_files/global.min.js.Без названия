function AddProductToShippingCart(e,t){var n=1,i=$("#qty_"+e+"_"+t);""!=i&&null!=i&&(n=parseInt(i.val()),isNaN(n)&&(n=1));var o=getNewSubmitForm();createNewFormElement(o,"ProductID",e),createNewFormElement(o,"VariantID",t),createNewFormElement(o,"Quantity",n),o.action="/addtocart.aspx",o.submit()}function getNewSubmitForm(){var e=document.createElement("FORM");return document.body.appendChild(e),e.method="POST",e}function createNewFormElement(e,t,n){var i=document.createElement("span");return i.innerHTML="<input type='hidden' name='"+t+"' value='"+n+"' />",e.appendChild(i),i}function removeSideTabsOnResize(){$(window).bind("resize",removeSideTabs)}function removeSideTabs(){var e="#counter, #liveHelp";$(window).width();$(window).width()<1300?$(e).hide():$(e).show()}$(document).ready(function(){function e(){t.animate({right:0==parseInt(t.css("right"),10)?-t.outerWidth():0})}var t=$("#liveHelp");if(t.length&&$(".liveHelpHandle").click(function(t){t.preventDefault,e()}),removeSideTabs(),removeSideTabsOnResize(),$(".categorymenu").hide(),$(".showMenu").length){$(".categorymenu").show();var n=$(".categorymenu").height(),i=1e3;i>n&&(n=i),$(".emptySpacer").css("height",n-118+"px")}else $(".categorymenu").hide(),$("#megamenu").hover(function(){$(".categorymenu").show()}),$(".megamenuwrapper").hover(function(){},function(){$(".categorymenu").hide()});$("#homeNews").length&&$("#homeNews").cycle({timeout:8e3,pager:"#newspager"}),$("#headerBanner").length&&$("#headerBanner").cycle({timeout:5e3}),$("#navbar .withDrop").each(function(){var e=this,t=$(this).find(".dropdown");$(e).find("a").first().append(' <img src="/App_Themes/Skin_1/newImages/icon-drop.png" width="14" height="14" align="absmiddle" />'),$(e).hover(function(){$(t).show(),$(e).addClass("hover")},function(){$(t).hide(),$(e).removeClass("hover")})}),$("#SearchText").keypress(function(e){13==e.which&&(e.preventDefault(),$(this).blur(),SubmitToOtherSite("1"))}),$("#SearchText1").keypress(function(e){13==e.which&&(e.preventDefault(),$(this).blur(),SubmitToOtherSite("0"))})}),$(window).load(function(){setTimeout(function(){$(".newToolStamp").fadeIn()},100)});var GC={cookie:{read:function(e){var t=new RegExp(e+"=([^;]+)"),n=t.exec(document.cookie);return null!=n?decodeURI(n[1]):null},create:function(e,t,n){if(null!=this.get(e))this.erase(e),this.set(e,t,n);else{var i="";if(n){var o=new Date;o.setTime(o.getTime()+24*n*60*60*1e3);var i="; expires="+o.toGMTString()}document.cookie=e+"="+t+i+";path=/;"}}},sidetab:{id:"#sidetab",init:function(){var e=$(this.id);e.find(".trigger").click(function(){e.hasClass("open")?GC.sidetab.close():GC.sidetab.open()})},open:function(){$(this.id).animate({right:0},{duration:300}).addClass("open")},close:function(){$(this.id).animate({right:-375},{duration:300}).removeClass("open")}}};window.location.origin||(window.location.origin=window.location.protocol+"//"+window.location.hostname+(window.location.port?":"+window.location.port:"")),$(window).bind("popstate",function(e){if(window.history.state&&"Product Search"===window.history.state.page){var t=GetParameterValues("view_type"),n=$.cookie("categoryView");t.toLowerCase()!=n.toLowerCase()&&$.cookie("categoryView",t,{expires:365}),LoadNextopiaResults()}});